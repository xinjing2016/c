#include <stdio.h>
void main(){
	//找字符，分类
	char str[]={"hgJGGF8 tyh 307"};
	int i1,i2,i3,i4,j;
	i1=i2=i3=i4=0;
	for (j=0;str[j]!='\0';j++){
		if (str[j] <='Z' && str[j]>='A'){i1++;}
		if (str[j] <='9' && str[j]>='0'){i2++;}
		if (str[j] <='z' && str[j]>='a'){i3++;}
		if (str[j] ==' '){i4++;}
		
		}printf("%3d%3d%3d%3d",i1,i2,i3,i4);
	
	
	
	
	
	
	
	}